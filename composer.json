{
    "name": "chengkangzai/chengkangzai.com",
    "type": "project",
    "description": "A personal website that serve as a personal profile",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "artesaos/seotools": "^v1.1.0",
        "barryvdh/laravel-translation-manager": "^0.6.5",
        "chengkangzai/laravel-apu-schedule": "^2.1.0",
        "filament/filament": "^2.17.55",
        "guzzlehttp/guzzle": "^7.8.1",
        "laravel-notification-channels/webhook": "^2.5",
        "laravel/fortify": "^1.20.0",
        "laravel/framework": "^10.42.0",
        "laravel/tinker": "^2.9.0",
        "league/flysystem-aws-s3-v3": "^3.22.0",
        "livewire/livewire": "^2.12.6",
        "silviolleite/laravelpwa": "^2.0.3",
        "spatie/laravel-backup": "^8.5.1",
        "spatie/laravel-permission": "^5.11.1",
        "spatie/laravel-remote": "^1.3.3",
        "spatie/laravel-sitemap": "^6.4.0",
        "spatie/laravel-tags": "^4.5.2",
        "spatie/laravel-translatable": "^6.5.5",
        "tanmuhittin/laravel-google-translate": "^2.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9.2",
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.23.1",
        "laravel/pint": "^1.13.10",
        "laravel/sail": "^1.27.2",
        "mockery/mockery": "^1.6.7",
        "nunomaduro/collision": "^7.10.0",
        "phpunit/phpunit": "^10.5.9",
        "spatie/laravel-ignition": "^2.4.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "run-ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --no-interaction",
            "@php artisan ide-helper:eloquent"
        ],
        "run-translation": [
            "@php artisan translations:reset",
            "@php artisan translations:import",
            "@php artisan translations:find"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
