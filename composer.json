{
    "name": "chengkangzai/chengkangzai.com",
    "type": "project",
    "description": "A personal website that serve as a personal profile, blog and a Covid Dashboard for Malaysia.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "artesaos/seotools": "^0.22.1",
        "barryvdh/laravel-translation-manager": "^0.6.3",
        "chengkangzai/laravel-apu-schedule": "^2.1.0",
        "filament/filament": "^2.16.68",
        "fruitcake/laravel-cors": "^2.2",
        "guzzlehttp/guzzle": "^7.5",
        "hammerstone/sidecar": "^0.3.12",
        "laravel-notification-channels/webhook": "^2.4",
        "laravel/fortify": "^1.16",
        "laravel/framework": "^9.50.2",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.12.2",
        "league/oauth2-client": "^2.6.1",
        "livewire/livewire": "^2.11.2",
        "microsoft/microsoft-graph": "^1.87",
        "saade/filament-fullcalendar": "^1.6",
        "silviolleite/laravelpwa": "^2.0.3",
        "spatie/browsershot": "^3.57.6",
        "spatie/laravel-backup": "^8.1.7",
        "spatie/laravel-permission": "^5.8",
        "spatie/laravel-remote": "^1.3.3",
        "spatie/laravel-sitemap": "^6.2.5",
        "spatie/laravel-tags": "^4.3.7",
        "spatie/laravel-translatable": "^6.3",
        "tanmuhittin/laravel-google-translate": "^2.1",
        "wire-elements/modal": "^1.0.7",
        "wnx/sidecar-browsershot": "^1.6"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.13",
        "chengkangzai/laravel-stubs": "^2.4.4",
        "fakerphp/faker": "^1.21.0",
        "laravel/pint": "^1.4.1",
        "laravel/sail": "^1.19.0",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^6.4",
        "orangehill/iseed": "^3.0.2",
        "phpunit/phpunit": "^9.6.3",
        "spatie/laravel-ignition": "^1.6.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "run-ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --no-interaction",
            "@php artisan ide-helper:eloquent"
        ],
        "run-translation": [
            "@php artisan translations:reset",
            "@php artisan translations:import",
            "@php artisan translations:find"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
