{
    "name": "chengkangzai/chengkangzai.com",
    "type": "project",
    "description": "A personal website that serve as a personal profile, blog and a Covid Dashboard for Malaysia.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "artesaos/seotools": "^v1.1.0",
        "barryvdh/laravel-translation-manager": "^0.6.5",
        "chengkangzai/laravel-apu-schedule": "^2.1.0",
        "filament/filament": "^2.17.50",
        "guzzlehttp/guzzle": "^7.7",
        "laravel-notification-channels/webhook": "^2.4",
        "laravel/fortify": "^1.17.4",
        "laravel/framework": "^10.14.1",
        "laravel/tinker": "^2.8.1",
        "league/flysystem-aws-s3-v3": "^3.15.0",
        "league/oauth2-client": "^2.7.0",
        "livewire/livewire": "^2.12.3",
        "microsoft/microsoft-graph": "^1.102",
        "saade/filament-fullcalendar": "^1.9.1",
        "silviolleite/laravelpwa": "^2.0.3",
        "spatie/laravel-backup": "^8.1.11",
        "spatie/laravel-permission": "^5.10.2",
        "spatie/laravel-remote": "^1.3.3",
        "spatie/laravel-sitemap": "^6.3.1",
        "spatie/laravel-tags": "^4.4.0",
        "spatie/laravel-translatable": "^6.5.2",
        "tanmuhittin/laravel-google-translate": "^2.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8.1",
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.23.0",
        "laravel/pint": "^1.10.3",
        "laravel/sail": "^1.23.0",
        "mockery/mockery": "^1.6.2",
        "nunomaduro/collision": "^7.7.0",
        "phpunit/phpunit": "^10.2.3",
        "spatie/laravel-ignition": "^2.2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "run-ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --no-interaction",
            "@php artisan ide-helper:eloquent"
        ],
        "run-translation": [
            "@php artisan translations:reset",
            "@php artisan translations:import",
            "@php artisan translations:find"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
