{
    "name": "chengkangzai/chengkangzai.com",
    "type": "project",
    "description": "A personal website that serve as a personal profile, blog and a Covid Dashboard for Malaysia.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "artesaos/seotools": "^0.22.0",
        "barryvdh/laravel-translation-manager": "^0.6.2",
        "chengkangzai/laravel-apu-schedule": "^2.0.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.4",
        "hammerstone/sidecar": "^0.3.9",
        "laravel-notification-channels/webhook": "^2.2",
        "laravel/fortify": "^1.7",
        "laravel/framework": "^9.0",
        "laravel/tinker": "^2.5",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/oauth2-client": "^2.6",
        "livewire/livewire": "^2.5",
        "microsoft/microsoft-graph": "^1.46",
        "silviolleite/laravelpwa": "^2.0",
        "spatie/browsershot": "^3.53",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-remote": "^1.2",
        "spatie/laravel-sitemap": "^6.0",
        "spatie/laravel-tags": "^4.0",
        "spatie/laravel-translatable": "^6.0",
        "tanmuhittin/laravel-google-translate": "^2.0",
        "wire-elements/modal": "^1.0",
        "wnx/sidecar-browsershot": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.10",
        "chengkangzai/laravel-stubs": "^2.3",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.1",
        "orangehill/iseed": "^3.0",
        "phpunit/phpunit": "^9.3.3",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "run-ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --no-interaction",
            "@php artisan ide-helper:eloquent"
        ],
        "run-translation": [
            "@php artisan translations:reset",
            "@php artisan translations:import",
            "@php artisan translations:find"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
